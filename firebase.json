{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "/testengine/",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "/files/:path",
        "destination": "https://cdn.kavisankar.com/files/:path"
      }
    ]
  }
}
